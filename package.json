{
  "name": "react-autosuggest",
  "version": "1.10.2",
  "description": "WAI-ARIA compliant React autosuggest component",
  "main": "dist/Autosuggest.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moroshko/react-autosuggest.git"
  },
  "author": "Misha Moroshko <michael.moroshko@gmail.com>",
  "bugs": {
    "url": "https://github.com/moroshko/react-autosuggest/issues"
  },
  "homepage": "https://github.com/moroshko/react-autosuggest",
  "scripts": {
    "start": "node server",
    "test": "BABEL_JEST_STAGE=0 jest",
    "dist": "npm test && babel src/Autosuggest.js -o dist/Autosuggest.js --stage 0 && babel src/sectionIterator.js -o dist/sectionIterator.js --stage 0",
    "build": "webpack --config webpack.dev.config.js",
    "deploy": "gh-pages-deploy"
  },
  "jest": {
    "rootDir": ".",
    "testDirectoryName": "tests",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "gh-pages-deploy": {
    "staticpath": "examples/dist",
    "prep": [
      "build"
    ],
    "noprompt": true
  },
  "dependencies": {
    "classnames": "^1.2.0",
    "debounce": "^1.0.0",
    "react": "^0.13.1"
  },
  "devDependencies": {
    "babel": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "babel-jest": "^5.0.1",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.10.1",
    "eslint": "^0.18.0",
    "eslint-loader": "^0.9.0",
    "extract-text-webpack-plugin": "^0.5.0",
    "gh-pages-deploy": "^0.3.0",
    "jest-cli": "^0.4.0",
    "json-loader": "^0.5.1",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "react-hot-loader": "^1.2.5",
    "style-loader": "^0.10.2",
    "webpack": "^1.8.5",
    "webpack-dev-server": "^1.8.0"
  },
  "keywords": [
    "autosuggest",
    "autocomplete",
    "auto-suggest",
    "auto-complete",
    "auto suggest",
    "auto complete",
    "react autosuggest",
    "react autocomplete",
    "react auto-suggest",
    "react auto-complete",
    "react auto suggest",
    "react auto complete",
    "react-autosuggest",
    "react-autocomplete",
    "react-auto-suggest",
    "react-auto-complete",
    "react-component"
  ],
  "license": "MIT"
}
